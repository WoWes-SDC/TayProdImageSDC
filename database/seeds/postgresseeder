const faker = require('faker');

//create fake product
const createFakeWhatever {

}

exports.seed = async function(knex, Promise) {
  let fakeProducts = [];
  const desiredProducts = 10000;

  for (let j = 0; j < 1000; j++) {
    for (let i = 0; i < desiredProducts; i++) {
      fakeProducts.push(createFakeProduct())
    }
    await knex('image_render')
    .insert(fakeProducts);
    setTimeout(()=> {
      fakeProducts = [];
    }, 0)
  }
}

//run command --> npx knex seed: run --env development (run from this location)